<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:wing="http://schemas.android.com/apk/res-auto"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/black"    android:orientation="vertical">    <com.android.mybarometer.DashboardView        android:id="@+id/barometerPanelView"        android:layout_width="match_parent"        android:layout_height="match_parent"        android:layout_gravity="center"        android:layout_margin="10dp"        android:layout_marginTop="10dp"        android:text="barometer_unit"        android:textSize="30dp"/>    <TextView        android:id="@+id/meter_value"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_gravity="center_horizontal"        android:layout_marginTop="20dp"        android:textSize="35dp" />    <TextView        android:id="@+id/altitude_value"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_gravity="center_horizontal"        android:layout_marginTop="8dp"        android:textSize="35dp" />    <TextView        android:id="@+id/altitude_calibration"        android:layout_width="wrap_content"        android:layout_height="wrap_content"        android:layout_gravity="center_horizontal"        android:layout_marginTop="28dp"        android:text="barometer_altitude_calibration"        android:textSize="18dp" /></LinearLayout>